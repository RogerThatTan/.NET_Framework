
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<form method="post">
    <input type="hidden" name="Id" value="@Model.Id" />
    <input type="text" value="@Model.Name" name="Name" placeholder="Name" class="form-control" />
    <input type="text" value="@Model.Cgpa" name="Cgpa" placeholder="Name" class="form-control" />
    <input type="submit" value="Update" class="btn btn-success" />

</form>